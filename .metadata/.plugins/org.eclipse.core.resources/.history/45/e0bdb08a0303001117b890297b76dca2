package work6feb;

class Animal {
	void sound() {
		System.out.println("Some Animal Sound");
	}

}

class Dog extends Animal {
	void sound() {
		System.out.println("Bark");
	}
}


	class Cat extends Animal {
		void sound() {
			System.out.println("Meow");
		}

	}
	
	public class Test{
		public static void main(String[] args) {
			Animal a1 = new Dog();
			Animal a2 = new Cat();
			
			a1.sound();
			a2.sound();
			
			
		}
	}

