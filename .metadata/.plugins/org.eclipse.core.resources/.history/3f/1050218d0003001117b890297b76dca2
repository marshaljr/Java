package work6feb;

class Student {
	void role() {
		System.out.println("I am student");
	}

}

class CollegeStudent extends Student {
	
	@Override
    void role() {
        System.out.println("I am a college student");
    }
	
	void display() {
//		System.out.println("I am college student");
		System.out.println("Display method of CollegeStudent - overriding");
	}

}
