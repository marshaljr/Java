package work4feb;

public class TestBankAccountSystem {

	public static void main(String[] args) {

		TestBankAccountSystem BankAccount = new TestBankAccountSystem();
		BankAccount.accountNumber = 101;
		BankAccount.balance = 500.00d;

		System.out.println("Account Number : " + BankAccount.accountNumber);
		System.out.println("Balance : " + BankAccount.balance);

	}

}
