package work4feb;

public class Student1 {
	private String name;
	private int rollNum;
	
//Default Constructor
	public void MyConstructor1(String n, int r) {
	
	}
	//Parameterized Constructor
	public void MyConstructor2(String n, int r) {
		this.name = n;
		this.rollNum = r;
	}
	
	public void setName(String n) {
		this.name = n;
		
	}
	public String getName() {
		return name;
		
	}
	

	void display() {
		System.out.println(name);
		System.out.println(rollNum);
	}

	public class Test1 {
		public static void main(String[] args) {
			int rollNum;

			Student1 ob = new Student1();
			
			ob.name = "AMit";
			rollNum = 101;
			
			ob.display();
			System.out.println(rollNum);

		}
	}

}
