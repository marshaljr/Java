package work4feb;

public class Student1 {
	String name;

	public void MyConstructor(String s) {
		this.name = s;
	}
	
	public void setName() {
		
	}
	public void getName() {
		
	}
	
	

	void display() {
		System.out.println(name);
	}

	public class Test1 {
		public static void main(String[] args) {
			int rollNum;

			Student1 ob = new Student1();
			
			ob.name = "AMit";
			rollNum = 101;
			
			ob.display();
			System.out.println(rollNum);

		}
	}

}
